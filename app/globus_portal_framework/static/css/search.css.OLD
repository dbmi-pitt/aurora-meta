
/* General */
html, body {
  font-family: museosans,"Helvetica Neue",Helvetica,Arial,sans-serif;
  font-weight: 300;
  font-size: 12px;
  color: #333;
}

h1 { font-size: 30px;  color:#fff;}
h2 { font-size: 26px; }
h3 { font-size: 18px; }
h5 { font-size: 16px; }
h2, h3, h4, h5, a {
    color: #708FCA;
    font-weight: 300;
}

hr.style1 {
    /* margin-top: 1rem; */
    /* margin-bottom: 1rem; */
    border: 0;
    border-top: 1px solid #bfc0c1;
}

hr.style18:before { 
  display: block; 
  content: ""; 
  height: 30px; 
  margin-top: -31px; 
  border-style: solid; 
  border-color: #8c8b8b; 
  border-width: 0 0 1px 0; 
  border-radius: 20px; 
}

div.info {
  font-family: "Gotham SSm A", "Gotham SSm B";  
