  margin-right: 100px;
  margin-top:  50px  
}

div.h-center {
  font-family: museosans,"Helvetica Neue",Helvetica,Arial,sans-serif;
  font-size: 16px;
  text-align: center;
  color: #2d528e;
  margin-top:  50px    
}

div.card {
